   <DataList id="MAPS">
      <name>Base Maps</name>
      <member visible="false">
         <DataItem>
            <name>MS Roads Imagery (Flat)</name>
            <dataProvider>
               <VirtualEarthProvider id="MS_ROADS_DATA">
                  <name>World Roads</name>
                  <layerId>roads</layerId>
                  <spatialExtent autoUpdate="#SCENE_01"/>
               </VirtualEarthProvider>
            </dataProvider>
            <style>
               <sld:TextureSymbolizer enabled="true">
                  <sld:Grid>
                     <sld:Dimensions>
                        <sld:Axis name="width">
                           <ogc:PropertyName>grid/row</ogc:PropertyName>
                        </sld:Axis>
                        <sld:Axis name="length">
                           <ogc:PropertyName>grid</ogc:PropertyName>
                        </sld:Axis>
                     </sld:Dimensions>
                     <sld:Geometry>
                        <sld:CssParameter name="geometry-x">
                           <ogc:PropertyName>grid/row/point/lon</ogc:PropertyName>
                        </sld:CssParameter>
                        <sld:CssParameter name="geometry-y">
                           <ogc:PropertyName>grid/row/point/lat</ogc:PropertyName>
                        </sld:CssParameter>
                     </sld:Geometry>
                  </sld:Grid>
                  <sld:Raster>
                     <sld:Dimensions>
                        <sld:Axis name="width">
                           <ogc:PropertyName>image/row</ogc:PropertyName>
                        </sld:Axis>
                        <sld:Axis name="height">
                           <ogc:PropertyName>image</ogc:PropertyName>
                        </sld:Axis>
                     </sld:Dimensions>
                     <sld:Opacity>1.0</sld:Opacity>
                     <sld:ChannelSelection>
                        <sld:RedChannel>
                           <sld:SourceChannelName>image/row/pixel/red</sld:SourceChannelName>
                        </sld:RedChannel>
                        <sld:GreenChannel>
                           <sld:SourceChannelName>image/row/pixel/green</sld:SourceChannelName>
                        </sld:GreenChannel>
                        <sld:BlueChannel>
                           <sld:SourceChannelName>image/row/pixel/blue</sld:SourceChannelName>
                        </sld:BlueChannel>
                     </sld:ChannelSelection>
                  </sld:Raster>
               </sld:TextureSymbolizer>
            </style>
            <style>
               <sld:GridBorderSymbolizer enabled="false" id="GRID_BORDER">
                  <sld:Dimensions>
                     <sld:Axis name="width">
                        <ogc:PropertyName>grid/row</ogc:PropertyName>
                     </sld:Axis>
                     <sld:Axis name="length">
                        <ogc:PropertyName>grid</ogc:PropertyName>
                     </sld:Axis>
                  </sld:Dimensions>
                  <sld:Geometry>
                     <sld:CssParameter name="geometry-x">
